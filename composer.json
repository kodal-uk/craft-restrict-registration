{
    "name": "kodal-uk/craft-restrict-registration",
    "description": "Restrict user registration by domain in Craft CMS, allows you to limit who can register for your site based on their email address.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "kodal",
        "restrict",
        "registration"
    ],
    "minimum-stability": "stable",
    "support": {
        "docs": "https://github.com/kodal-uk/craft-restrict-registration/blob/master/README.md",
        "issues": "https://github.com/kodal-uk/craft-restrict-registration/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Jermey",
            "homepage": "https://github.com/seanjermey"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-beta.23"
    },
    "autoload": {
        "psr-4": {
          "Kodal\\RestrictRegistration\\": "src/"
        }
    },
    "extra": {
        "name": "Restrict Registration",
        "handle": "restrict-registration",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/kodal-uk/craft-restrict-registration/blob/master/CHANGELOG.md",
        "class": "Kodal\\RestrictRegistration\\Plugin"
    }
}
